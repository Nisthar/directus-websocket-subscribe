{
	"name": "directus-websocket-subscribe",
	"version": "2.0.0",
	"author": "brainslug",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-hook",
		"directus-subscribe",
		"directus-websocket"
	],
	"directus:extension": {
		"type": "hook",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^9.4.1",
		"hidden": false
	},
	"scripts": {
		"build": "directus-extension build",
		"docker": "docker-compose up -d",
		"dev-build": "directus-extension build && docker-compose restart directus",
		"docker-stop": "docker-compose down"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "^9.4.1",
		"@types/uuid": "^8.3.3",
		"@types/ws": "^8.2.2"
	},
	"dependencies": {
		"uuid": "^8.3.2",
		"ws": "^8.3.0"
	}
}
