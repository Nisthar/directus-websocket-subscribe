{
	"name": "directus-websocket-subscribe",
	"version": "2.0.0",
	"author": "brainslug",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-hook"
	],
	"directus:extension": {
		"type": "hook",
		"path": "dist/index.js",
		"source": "src/index.ts",
